<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.RazorWallpapers" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#6B4F9E</item>
        <item name="colorPrimaryVariant">@color/ic_launcher_background</item>
        <item name="colorOnPrimary">@color/ic_launcher_background</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#4DA8D1</item>
        <item name="colorSecondaryVariant">@color/purple_200</item>
        <item name="colorOnSecondary">@color/purple_500</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.RazorWallpapers.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>


    <style name="Theme.RazorWallpapers.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RazorWallpapers.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>